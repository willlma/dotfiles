{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true
  },
  "rules": {
    "semi": [2, "always"],
    "quotes": [1, "single"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "curly": [0, "multi"],
    "space-infix-ops": 0,
    "comma-spacing": 1,
    "no-trailing-spaces": 0,
    "consistent-return": 0,
    "max-len": [1, 82, 4],
    "max-nested-callbacks": [1, 3],
    "strict": 0,
    "no-undef": 1,
    "no-console": 0,
    "no-shadow": 1,
    "semi-spacing": 0,
    "no-labels": 0,
    "no-constant-condition": 1,
    "no-new": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "utility": false,
    "window": false,
    "document": false,
    "location": false
  },
  "env": {
    "browser": true
  }
}
