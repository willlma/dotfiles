"*":
  "atom-ide-ui":
    hyperclick:
      darwinTriggerKeys: "altKey,metaKey"
    use:
      "atom-ide-busy-signal": "default"
      "atom-ide-code-actions": "default"
      "atom-ide-code-format": "default"
      "atom-ide-code-highlight": "default"
      "atom-ide-console": "default"
      "atom-ide-datatip": "default"
      "atom-ide-debugger": "default"
      "atom-ide-definitions": "default"
      "atom-ide-diagnostics": "default"
      "atom-ide-diagnostics-ui": "default"
      "atom-ide-find-references": "default"
      "atom-ide-global": "default"
      "atom-ide-outline-view": "default"
      "atom-ide-signature-help": "default"
      "atom-ide-terminal": "default"
      hyperclick: "default"
  core:
    disabledPackages: [
      "linter-flow"
      "busy-signal"
      "flow-ide"
      "goto-definition"
      "hyperclick"
      "intentions"
      "atom-ide-ui"
      "language-javascript"
      "language-typescript"
      "linter-rubocop"
    ]
    packagesWithKeymapsDisabled: [
      "todo-show"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    showInvisibles: true
    tabType: "soft"
  "exception-reporting":
    userId: "699ddf69-2b01-49b0-a969-d7aad520e26b"
  "highlight-selected":
    hideHighlightOnSelectedWord: true
    highlightBackground: true
    lightTheme: true
    minimumLength: 1
    onlyHighlightWholeWords: false
    showResultsOnScrollBar: true
  "language-babel":
    emmetCSSAutocomplete: true
  linter: {}
  "linter-eslint": {}
  "linter-flow":
    executablePath: "node_modules/.bin/flow"
  "linter-rubocop": {}
  "linter-ui-default":
    panelHeight: 19
    showPanel: true
  "markdown-preview":
    breakOnSingleNewline: true
  "markdown-preview-plus": {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "prettier-atom":
    formatOnSaveOptions: {}
    prettierEslintOptions:
      prettierLast: true
    prettierOptions:
      arrowParens: "always"
      jsxBracketSameLine: true
      singleQuote: true
    useEslint: true
  tabs:
    addNewTabsAtEnd: true
    alwaysShowTabBar: false
    enableMruTabSwitching: false
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  "wrap-with-anything":
    chars: "*~/_{"
